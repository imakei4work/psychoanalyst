{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1
  },
  "parserOptions":{
    "ecmaVersion":10,
    "sourceType":"module"
  },
  "parser":"babel-eslint"
}